{
  "name": "@splinetool/react-spline",
  "version": "3.0.1",
  "packageManager": "yarn@3.3.0",
  "files": [
    "dist"
  ],
  "type": "module",
  "react-native": "./dist/react-spline.js",
  "exports": {
    ".": {
      "types": "./dist/Spline.d.ts",
      "import": "./dist/react-spline.js",
      "require": "./dist/react-spline.cjs"
    }
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "serve": "vite preview",
    "build": "vite build && yarn build-types",
    "build-types": "tsc --skipLibCheck --isolatedModules false --noEmit false --emitDeclarationOnly --declaration --outDir dist/",
    "publish": "yarn build && np --no-tests"
  },
  "peerDependencies": {
    "@splinetool/runtime": "*",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@splinetool/runtime": "^1.3.10",
    "@types/animejs": "^3.1.7",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "animejs": "^3.2.1",
    "modern-normalize": "^1.1.0",
    "np": "^10.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "react-merge-refs": "^2.1.1"
  }
}
